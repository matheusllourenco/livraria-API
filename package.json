{
  "name": "livraria",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/app.js",
    "dev": "nodemon ./src/app.js",
    "generate:migration": "npx sequelize-cli migration:generate --name",
    "execute:migration": "npx sequelize-cli db:migrate",
    "undo:migration": "npx sequelize-cli db:migrate:undo"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.11",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.19"
  }
}
